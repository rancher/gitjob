{
  "linters": {
    "disable-all": true,
    "enable": [
      "govet",
      "unparam",
      "nakedret",
      "dupl",
      "goimports",
      "misspell",
      "ineffassign",
      "revive",
      "gosec",
      "typecheck",
      "unused"
    ]
  },
  "linters-settings": {
    "revive": {
      "rules": [
        {
          "name": "blank-imports",
          "severity": "warning"
        },
        {
          "name": "unexported-return",
          "severity": "warning"
        }
      ]
    }
  },
  "run": {
    "skip-files": [
      "/zz_generated_"
    ],
    "deadline": "5m"
  }
}
